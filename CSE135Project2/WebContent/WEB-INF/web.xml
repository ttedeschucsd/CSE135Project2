<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>signup</servlet-name>
		<jsp-file>/jsp/signup.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<jsp-file>/jsp/login.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>categories</servlet-name>
		<jsp-file>/jsp/categories.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>products</servlet-name>
		<jsp-file>/jsp/products.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>purchase</servlet-name>
		<jsp-file>/jsp/purchase.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>browse</servlet-name>
		<jsp-file>/jsp/browsing.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>product-order</servlet-name>
		<jsp-file>/jsp/product-order.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>analytics</servlet-name>
		<jsp-file>/jsp/analytics.jsp</jsp-file>
	</servlet>
	<servlet>
		<servlet-name>confirm</servlet-name>
		<jsp-file>/jsp/confirmation.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>signup</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>categories</servlet-name>
		<url-pattern>/categories</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>products</servlet-name>
		<url-pattern>/products</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>purchase</servlet-name>
		<url-pattern>/purchase</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>browse</servlet-name>
		<url-pattern>/browse</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>product-order</servlet-name>
		<url-pattern>/product-order</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>confirm</servlet-name>
		<url-pattern>/confirm</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>analytics</servlet-name>
		<url-pattern>/analytics</url-pattern>
	</servlet-mapping>
</web-app>
